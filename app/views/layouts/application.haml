!!! Strict
%html
  %head
    %meta{'http-equiv' => 'Content-Type', :content => "text/html; charset=iso-8859-1"}
    %title
      = @title ||= "Dove Tail Timbers"
    %meta{:name => "Description", :content => ""}
    %meta{:name => "Abstract", :content => ""}
    %meta{:name => "Keywords", :content => ""}
    = stylesheet_link_tag 'print.css', :media => 'print'
    = stylesheet_link_tag 'screen.css', 'application.css', 'nav.css', :media => 'screen, projection'
    /[if lt IE 8]
      = stylesheet_link_tag 'ie.css', :media => 'screen, projection'
      
    = javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.1/jquery.min.js", 'transitions.js', 'hoverIntent.js', 'superfish.js', 'application.js'
     
  %body.index
    .container
      #header
        %h1 Dove Tail Timbers
      #nav
        = yield :nav
      #feature
        = yield :feature
      #content.padding
        = yield
      #footer
        = yield :footer
    = render :partial => "layouts/analytics"
        


        
        
          
          
          
          
          
          
          
          
          
          
